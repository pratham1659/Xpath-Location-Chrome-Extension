{
  "manifest_version": 3,
  "name": "XPath Locator",
  "version": "1.0",
  "description": "Find XPath of selected element on the webpage.",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
